{
	"AllowedHosts": "*",
	"Logging": {
		"MinimumLevel": {
			"Default": "Information"
		},
		"LogLevel": {
			"Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None"
		}
	},
	"ApplicationInsights": {
		"InstrumentationKey": "",
		"ConnectionString": ""
	},
	"ConnectionStrings": {
		"atmdb": "Server=.;Database=atmdb;Trusted_Connection=true;MultipleActiveResultSets=true;Integrated Security=true"
	},
	"Sieve": {
		"MaxPageSize": 50,
		"DefaultPageSize": 10,
		"CaseSensitive": false,
		"ThrowExceptions": true,
		"IgnoreNullsOnNotEqual": true
	}
}
